{
    "name": "twitch-hud-labyrinth",
    "productName": "Twitch Hud Labyrinth",
    "description": "Custom Twitch HUD Labyrinth game.",
    "version": "1.0.0",
    "author": "Brandon Lee Kitajchuk",
    "private": true,
    "main": "hud/main.js",
    "scripts": {
        "start": "./node_modules/.bin/nodemon -i ./hud/source -i ./hud/public -i ./hud/views -i ./hud/json -i ./node_modules ./hud/main.js",
        "start:prod": "./node_modules/.bin/pm2 -a -p pid.log -l pm2.log -o out.log -e err.log start ./hud/main.js",
        "build": "./node_modules/.bin/webpack --env.production --progress --profile --display-modules --colors",
        "watch": "./node_modules/.bin/webpack --env.sandbox --hide-modules --colors --watch",
        "deploy": "rsync -av -e 'ssh' --rsync-path 'sudo rsync' hud package.json ec2-user@54.69.44.146:/var/www/html/",
        "connect": "ssh ec2-user@54.69.44.146",
        "postinstall": "cd ./node_modules/properjs-hobo && npm install && npm run build -- 'is eq not one next prev attr last first index parent filter detach append remove trigger prepend closest children removeAttr toggleClass'"
    },
    "repository": "git@github.com:kitajchuk/twitch-hud-labyrinth.git",
    "license": "CC0-1.0",
